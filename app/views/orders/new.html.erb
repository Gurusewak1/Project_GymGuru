<%= form_with(model: @order, url: orders_path, method: :post) do |form| %>
  <%= form.label :total_amount %>
  <%= form.text_field :total_amount %>

  <%= form.label :status %>
  <%= form.text_field :status %>

  <%= form.label :address %>
  <%= form.text_field :address %>

  <%= form.label :province %>
  <%= form.text_field :province %>

  <%= form.submit "Create Order" %>
<% end %>
